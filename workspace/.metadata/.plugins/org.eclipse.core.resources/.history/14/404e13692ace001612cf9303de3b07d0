import cx_Oracle


def make_Connection() :#
    try:
        conn = cx_Oracle.connect('sparkmaya/sparkmaya@127.0.0.1/XE')
        valid_connection(conn)
        get_tables_metadata(conn)
        conn.close()
    except:
        print("Failed to connect to SparkMaya Master Database")


if __name__ == '__main__':
    make_Connection()