import cx_Oracle

def make_Connection() :
    con = cx_Oracle.connect('sparkmaya/sparkmaya@127.0.0.1/XE')
    print con.version
    cur = con.cursor()
    connection = cur.execute('select * from connection_detail where valid_connection=0')
    
    if len(con)>0:
        println("Connection Valid..")
        for result in cur:
            print "Updating %a wth valid_conection=1" %(result[7])
            print result

    cur.close()
    con.close()




if __name__ == '__main__':
    make_Connection()